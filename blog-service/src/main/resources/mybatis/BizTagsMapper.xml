<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper
	namespace="com.shengdingbox.blog.persistence.mapper.BizTagsMapper">
	<resultMap id="rm"
		type="com.shengdingbox.blog.persistence.beans.BizTags">
		<result property="id" jdbcType="BIGINT" column="id" />
		<result property="name" jdbcType="VARCHAR" column="name" />
		<result property="description" jdbcType="VARCHAR"
			column="description" />
		<result property="createTime" jdbcType="TIMESTAMP"
			column="create_time" />
		<result property="updateTime" jdbcType="TIMESTAMP"
			column="update_time" />
	</resultMap>

	<select id="findPageBreakByCondition"
		parameterType="com.shengdingbox.blog.business.vo.TagsConditionVO"
		resultMap="rm">
		SELECT
		t.*
		FROM
		biz_tags t
		WHERE
		1 = 1
	</select>
</mapper>

